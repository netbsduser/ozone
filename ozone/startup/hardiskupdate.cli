#
#  Copy updated files from baron
#
credir OZ_LOAD_FS:/binutils/binaries/
credir OZ_LOAD_FS:/gcc/binaries/
credir OZ_LOAD_FS:/gcc/libraries/
credir OZ_LOAD_FS:/gcc/includes/
credir OZ_LOAD_FS:/ozone/binaries/
credir OZ_LOAD_FS:/ozone/includes/
credir OZ_LOAD_FS:/ozone/libraries/
credir OZ_LOAD_FS:/ozone/startup/
